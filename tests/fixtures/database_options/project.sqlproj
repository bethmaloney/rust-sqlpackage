<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build">
  <Sdk Name="Microsoft.Build.Sql" Version="2.0.0" />
  <PropertyGroup>
    <Name>DatabaseOptions</Name>
    <DSP>Microsoft.Data.Tools.Schema.Sql.Sql160DatabaseSchemaProvider</DSP>
    <DefaultCollation>Latin1_General_CI_AS</DefaultCollation>
    <DefaultFilegroup>PRIMARY</DefaultFilegroup>
    <PageVerify>CHECKSUM</PageVerify>
    <AnsiNullDefaultOn>True</AnsiNullDefaultOn>
    <AnsiNullsOn>True</AnsiNullsOn>
    <AnsiWarningsOn>True</AnsiWarningsOn>
    <ArithAbortOn>True</ArithAbortOn>
    <ConcatNullYieldsNullOn>True</ConcatNullYieldsNullOn>
    <FullTextEnabled>True</FullTextEnabled>
  </PropertyGroup>
</Project>
